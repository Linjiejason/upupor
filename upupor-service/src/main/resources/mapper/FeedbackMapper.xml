<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.upupor.service.data.dao.mapper.FeedbackMapper">
  <resultMap id="BaseResultMap" type="com.upupor.service.data.dao.entity.Feedback">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="user_id" jdbcType="VARCHAR" property="userId" />
    <result column="feedback_id" jdbcType="VARCHAR" property="feedbackId" />
    <result column="status" jdbcType="INTEGER" property="status" />
    <result column="reply" jdbcType="VARCHAR" property="reply" />
    <result column="create_time" jdbcType="BIGINT" property="createTime" />
    <result column="sys_update_time" jdbcType="TIMESTAMP" property="sysUpdateTime" />
  </resultMap>
  <resultMap extends="BaseResultMap" id="ResultMapWithBLOBs" type="com.upupor.service.data.dao.entity.Feedback">
    <result column="feedback_content" jdbcType="LONGVARCHAR" property="feedbackContent" />
  </resultMap>
  <sql id="Base_Column_List">
    id, user_id, feedback_id, status, reply, create_time, sys_update_time
  </sql>
  <sql id="Blob_Column_List">
    feedback_content
  </sql>

</mapper>