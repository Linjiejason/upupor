<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.upupor.data.dao.mapper.ContentExtendMapper">
    <resultMap id="BaseResultMap" type="com.upupor.data.dao.entity.ContentExtend">
        <result column="content_id" jdbcType="VARCHAR" property="contentId"/>
        <result column="detail_content" jdbcType="VARCHAR" property="detailContent"/>
        <result column="sys_update_time" jdbcType="TIMESTAMP" property="sysUpdateTime"/>
    </resultMap>
    <sql id="Base_Column_List">
    content_id, sys_update_time,detail_content
  </sql>
</mapper>